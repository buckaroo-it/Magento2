<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job instance="Buckaroo\Magento2\Cron\CancelExpiredOrders" method="execute" name="buckaroo_magento2_cancel_cron">
            <schedule>0 */6 * * *</schedule>
        </job>

        <job instance="Buckaroo\Magento2\Cron\LogCleaner" method="execute" name="buckaroo_magento2_log_cleaner">
            <schedule>17 * * * *</schedule>
        </job>
    </group>
</config>
