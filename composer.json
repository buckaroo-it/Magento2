{
    "name": "buckaroo/magento2",
    "description": "Buckaroo Magento 2 extension",
    "require": {
        "php": "~7.4.0||~8.1.0||~8.2.0||~8.3.0||~8.4.0",
        "magento/module-payment": ">=100.3.3",
        "magento/framework": ">=103.0.0"
    },
    "type": "magento2-module",
    "license": "CC-BY-NC-ND-3.0",
    "authors": [
        {
            "name": "Buckaroo",
            "email": "support@buckaroo.nl",
            "homepage": "https://www.buckaroo.nl"
        }
    ],
    "support": {
        "email": "support@buckaroo.nl",
        "issues": "https://support.buckaroo.nl/contact",
        "wiki": "https://support.buckaroo.nl",
        "docs": "https://docs.buckaroo.io/"
    },
    "homepage": "https://www.buckaroo.nl",
    "version" : "v1.53.2",
    "minimum-stability": "stable",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Buckaroo\\Magento2\\": ""
        }
    },
    "require-dev": {
        "php-webdriver/webdriver": "^1.15.2",
        "squizlabs/php_codesniffer": "^3.13.4",
        "phpmd/phpmd": "^2.15.0",
        "phpstan/phpstan": "^1.12.32",
        "magento/magento-coding-standard": "^38",
        "friendsofphp/php-cs-fixer": "^3.22",
        "rector/rector": "^1.0",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}

